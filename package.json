{
  "name": "react-native-typeskill",
  "version": "0.10.0-alpha.20",
  "repository": "git@github.com:jsamr/react-native-typeskill.git",
  "description": "Operational-Transform Based (React) Native Rich Text Editor",
  "author": "Jules Randolph <jules.sam.randolph@gmail.com>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "autolint": "tslint 'src/**/*?(.d).ts?(x)' --fix",
    "build": "npm run build:clean; npm run build:types && npm run build:js",
    "build:clean": "rimraf lib",
    "build:types": "ttsc --project tsconfig-build.json",
    "build:js": "babel src --config-file ./build.babel.config.js --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "prepare": "npm run build",
    "test": "npm run validate:typescript && npm run validate:lint && npm run test:jest",
    "test:jest": "jest",
    "validate:typescript": "tsc --project ./ --noEmit",
    "validate:lint": "eslint --ext .ts --ext .tsx 'src/'",
    "release": "standard-version",
    "semantic-release": "semantic-release"
  },
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "react native",
    "rich text",
    "editor",
    "redactor",
    "text",
    "typeskill",
    "quilljs",
    "delta",
    "expo"
  ],
  "dependencies": {
    "@types/diff": "^4.0.2",
    "@types/invariant": "^2.2.29",
    "@types/ramda": "^0.26.9",
    "autobind-decorator": "~2.4.0",
    "diff": "~4.0.1",
    "eventemitter3": "~3.1.2",
    "invariant": "~2.2.4",
    "prop-types": "~15.7.2",
    "quill-delta": "~4.2.1",
    "ramda": "~0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-destructuring": "^7.4.4",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.5",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@react-native-community/eslint-config": "^0.0.5",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.9",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.18",
    "@types/react-native": "^0.57.60",
    "@types/react-test-renderer": "^16.8.1",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "@zerollup/ts-transform-paths": "^1.7.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-jest": "^24.6.0",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-native": "^0.59.8",
    "react-test-renderer": "^16.8.6",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.14",
    "standard-version": "^6.0.1",
    "ttypescript": "^1.5.6",
    "typescript": "^3.3.4000"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
